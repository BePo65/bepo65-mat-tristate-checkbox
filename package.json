{
  "name": "bepo65-mat-tristate-checkbox",
  "version": "15.1.2",
  "description": "3-state checkbox for Angular with Material as library component with demo project",
  "repository": {
    "type": "git",
    "url": "https://github.com/BePo65/mat-tristate-checkbox.git"
  },
  "author": "Bernhard Pottler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BePo65/mat-tristate-checkbox/issues"
  },
  "homepage": "https://github.com/BePo65/mat-tristate-checkbox",
  "scripts": {
    "ng": "ng",
    "lint": "npm run lint:root && npm run lint:lib && npm run lint:demo",
    "lint:root": "eslint --report-unused-disable-directives --config=./.eslintrc.root.json .",
    "lint:lib": "eslint --report-unused-disable-directives \"projects/bepo65/mat-tristate-checkbox\"",
    "lint:demo": "eslint --report-unused-disable-directives \"projects/mat-tristate-checkbox-demo\"",
    "lint:cli": "eslint --report-unused-disable-directives .eslintrc.json \"./projects/**/{src,e2e}/**/*.{ts,js,json}\"",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:commits": "commitlint --from 6c5f03e84b9b0 --to HEAD --verbose",
    "test": "npm run test:lib -- --watch=false && npm run test:demo -- --watch=false",
    "test:lib": "ng test @bepo65/mat-tristate-checkbox",
    "test:lib:coverage": "set BROWSERSLIST_IGNORE_OLD_DATA=1 && ng test @bepo65/mat-tristate-checkbox --code-coverage --watch=false",
    "test:lib:coverage:ui": "set BROWSERSLIST_IGNORE_OLD_DATA=1 && ng test @bepo65/mat-tristate-checkbox --code-coverage --watch=false && node tools/open-coverage.mjs",
    "test:demo": "ng test mat-tristate-checkbox-demo",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "ng build @bepo65/mat-tristate-checkbox",
    "build:lib:prod": "ng build @bepo65/mat-tristate-checkbox --configuration production && cpy CHANGELOG.md dist/bepo65/mat-tristate-checkbox/",
    "build:demo": "ng build mat-tristate-checkbox-demo",
    "init-changelog": "standard-version -i CHANGELOG.md --same-file --prerelease pre --first-release",
    "release": "standard-version -i CHANGELOG.md --same-file",
    "start": "ng serve --open",
    "e2e": "npm run cypress:demo:run:e2e",
    "ct": "npm run cypress:demo:run:ct && npm run cypress:lib:run:ct",
    "cypress:demo:open": "start-server-and-test \"ng serve\" http://localhost:4200 \"cypress open --project=projects/mat-tristate-checkbox-demo\"",
    "cypress:demo:run:e2e": "start-server-and-test \"ng serve\" http://localhost:4200 \"cypress run --project=projects/mat-tristate-checkbox-demo\"",
    "cypress:demo:run:ct": "cypress run --project=projects/mat-tristate-checkbox-demo --component",
    "cypress:lib:open": "cypress open --project=projects/bepo65/mat-tristate-checkbox",
    "cypress:lib:run:ct": "cypress run --project=projects/bepo65/mat-tristate-checkbox --component"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "roboto-fontface": "^0.10.0",
    "rxjs": "^7.8.1",
    "zone.js": "^0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.8",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "^15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@angular/cli": "^15.2.8",
    "@angular/compiler-cli": "^15.2.9",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-angular": "^17.6.3",
    "@cypress/schematic": "^2.5.0",
    "@cypress/webpack-preprocessor": "^5.17.1",
    "@types/jasmine": "^4.3.1",
    "@types/node": "^20.2.3",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "cpy-cli": "^4.2.0",
    "cypress": "^12.13.0",
    "eslint": "^8.41.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-jsdoc": "^44.2.5",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^8.0.3",
    "jasmine-core": "^4.6.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "material-icons": "^1.13.7",
    "ng-packagr": "^15.2.2",
    "standard-version": "^9.5.0",
    "start-server-and-test": "^2.0.0",
    "stylelint": "^15.6.2",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylus": "^0.59.0",
    "terser": "^5.17.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "standard-version": {
    "packageFiles": [
      {
        "filename": "package.json",
        "type": "json"
      }
    ],
    "bumpFiles": [
      {
        "filename": "package.json",
        "type": "json"
      },
      {
        "filename": "projects/bepo65/mat-tristate-checkbox/package.json",
        "type": "json"
      },
      {
        "filename": "README.md",
        "updater": "tools/npm-scripts/readme-updater.js"
      },
      {
        "filename": "projects/bepo65/mat-tristate-checkbox/README.md",
        "updater": "tools/npm-scripts/readme-updater.js"
      }
    ]
  }
}
